<?xml version="1.0" encoding="UTF-8"?>
<XMLTooling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:test="http://www.example.org/testObjects" xmlns="http://www.opensaml.org/xmltooling-config">
    
    <ObjectProviders>
        
        <!-- SimpleXMLObject object used for testing -->
        <ObjectProvider qualifiedName="test:SimpleElement">
            <BuilderClass className="org.opensaml.xml.mock.SimpleXMLObjectBuilder"/>
            <MarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectMarshaller"/>
            <UnmarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectUnmarshaller"/>
        </ObjectProvider>
        
        <!-- SimpleXMLObject object used for testing -->
        <ObjectProvider qualifiedName="test:Response">
            <BuilderClass className="org.opensaml.xml.mock.SimpleXMLObjectBuilder"/>
            <MarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectMarshaller"/>
            <UnmarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectUnmarshaller"/>
        </ObjectProvider>
        
        <!-- SimpleXMLObject object used for testing -->
        <ObjectProvider qualifiedName="test:Statement">
            <BuilderClass className="org.opensaml.xml.mock.SimpleXMLObjectBuilder"/>
            <MarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectMarshaller"/>
            <UnmarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectUnmarshaller"/>
        </ObjectProvider>
        
        <!-- Default object provider impl for testing -->
        <ObjectProvider qualifiedName="DEFAULT">
            <BuilderClass className="org.opensaml.xml.schema.impl.XSAnyBuilder"/>
            <MarshallingClass className="org.opensaml.xml.schema.impl.XSAnyMarshaller"/>
            <UnmarshallingClass className="org.opensaml.xml.schema.impl.XSAnyUnmarshaller"/>
        </ObjectProvider>
        
    </ObjectProviders>

</XMLTooling>
