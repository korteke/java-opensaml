<?xml version="1.0" encoding="UTF-8"?>
<XMLTooling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.opensaml.org/xmltooling-config ../../src/schema/xmltooling-config.xsd"
    xmlns:wsa="http://www.w3.org/2005/08/addressing"
    xmlns="http://www.opensaml.org/xmltooling-config">

    <!-- WS-Addressing 1.0 Object Providers -->
    <ObjectProviders>
    
        <!-- wsa:Action -->
        <ObjectProvider qualifiedName="wsa:Action">
            <BuilderClass
                className="org.opensaml.ws.wsaddressing.impl.ActionBuilder" />
            <MarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.ActionMarshaller" />
            <UnmarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.ActionUnmarshaller" />
        </ObjectProvider>

        <!-- wsa:Address -->
        <ObjectProvider qualifiedName="wsa:Address">
            <BuilderClass
                className="org.opensaml.ws.wsaddressing.impl.AddressBuilder" />
            <MarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.AddressMarshaller" />
            <UnmarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.AddressUnmarshaller" />
        </ObjectProvider>

        <!-- wsa:EndpointReference -->
        <ObjectProvider qualifiedName="wsa:EndpointReference">
            <BuilderClass
                className="org.opensaml.ws.wsaddressing.impl.EndpointReferenceBuilder" />
            <MarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.EndpointReferenceMarshaller" />
            <UnmarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.EndpointReferenceUnmarshaller" />
        </ObjectProvider>

        <!-- wsa:Metadata -->
        <ObjectProvider qualifiedName="wsa:Metadata">
            <BuilderClass
                className="org.opensaml.ws.wsaddressing.impl.MetadataBuilder" />
            <MarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.MetadataMarshaller" />
            <UnmarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.MetadataUnmarshaller" />
        </ObjectProvider>

        <!-- wsa:ReferenceParameters -->
        <ObjectProvider qualifiedName="wsa:ReferenceParameters">
            <BuilderClass
                className="org.opensaml.ws.wsaddressing.impl.ReferenceParametersBuilder" />
            <MarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.ReferenceParametersMarshaller" />
            <UnmarshallingClass
                className="org.opensaml.ws.wsaddressing.impl.ReferenceParametersUnmarshaller" />
        </ObjectProvider>

    </ObjectProviders>

</XMLTooling>
