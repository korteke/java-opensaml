<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
    <local-check-config name="Internet 2" location="checkstyle.xml" type="project" description="Code standards for Internet 2 code">
        <additional-data name="protect-config-file" value="true"/>
    </local-check-config>
    <fileset name="all" enabled="true" check-config-name="Internet 2" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
    </fileset>
    <filter name="FileTypesFilter" enabled="true">
        <filter-data value="java"/>
    </filter>
</fileset-config>
