<?xml version="1.0" encoding="UTF-8"?>
<xt:XMLTooling xmlns:xt="http://www.opensaml.org/xmltooling-config" 
               xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://www.opensaml.org/xmltooling-config ../../../../../src/schema/xmltooling-config.xsd ">

  <xt:ObjectProviders>
    <xt:ObjectProvider qualifiedName="ds:Signature">
      <xt:BuilderClass className="org.opensaml.xml.signature.impl.XMLSecSignatureBuilder"/>
      <xt:MarshallingClass className="org.opensaml.xml.signature.impl.XMLSecSignatureMarshaller"/>
      <xt:UnmarshallingClass className="org.opensaml.xml.signature.impl.XMLSecSignatureUnmarshaller"/>
    </xt:ObjectProvider>
  </xt:ObjectProviders>
  
</xt:XMLTooling>
