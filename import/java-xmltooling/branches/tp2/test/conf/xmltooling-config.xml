<?xml version="1.0" encoding="UTF-8"?>
<XMLTooling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.opensaml.org/xmltooling-config ../../src/schema/xmltooling-config.xsd"
 xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
 xmlns:test="http://www.example.org/testObjects"
 xmlns="http://www.opensaml.org/xmltooling-config">
    
    <ObjectProviders>
    
        <!-- XML Digital Signature -->
        <ObjectProvider qualifiedName="ds:Signature">
            <BuilderClass className="org.opensaml.xml.signature.SignatureBuilder" />
            <MarshallingClass className="org.opensaml.xml.signature.SignatureMarshaller" />
            <UnmarshallingClass className="org.opensaml.xml.signature.SignatureUnmarshaller" />
        </ObjectProvider>
        
        <!-- XML Digital Signature KeyInfo -->
        <ObjectProvider qualifiedName="ds:KeyInfo">
            <BuilderClass className="org.opensaml.xml.signature.KeyInfoBuilder" />
            <MarshallingClass className="org.opensaml.xml.signature.KeyInfoMarshaller" />
            <UnmarshallingClass className="org.opensaml.xml.signature.KeyInfoUnmarshaller" />
        </ObjectProvider>
        
        <!-- SimpleXMLObject object used for testing -->
        <ObjectProvider qualifiedName="test:SimpleElement">
            <BuilderClass className="org.opensaml.xml.mock.SimpleXMLObjectBuilder" />
            <MarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectMarshaller" />
            <UnmarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectUnmarshaller" />
        </ObjectProvider>
        
        <!-- SimpleXMLObject object used for testing -->
        <ObjectProvider qualifiedName="test:Response">
            <BuilderClass className="org.opensaml.xml.mock.SimpleXMLObjectBuilder" />
            <MarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectMarshaller" />
            <UnmarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectUnmarshaller" />
        </ObjectProvider>
        
        <!-- SimpleXMLObject object used for testing -->
        <ObjectProvider qualifiedName="test:Statement">
            <BuilderClass className="org.opensaml.xml.mock.SimpleXMLObjectBuilder" />
            <MarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectMarshaller" />
            <UnmarshallingClass className="org.opensaml.xml.mock.SimpleXMLObjectUnmarshaller" />
        </ObjectProvider>
        
        <!-- SimpleXMLObject object used for testing -->
        <ObjectProvider qualifiedName="DEFAULT">
            <BuilderClass className="org.opensaml.xml.ElementProxyBuilder" />
            <MarshallingClass className="org.opensaml.xml.ElementProxyMarshaller" />
            <UnmarshallingClass className="org.opensaml.xml.ElementProxyUnmarshaller" />
        </ObjectProvider>
        
    </ObjectProviders>

</XMLTooling>