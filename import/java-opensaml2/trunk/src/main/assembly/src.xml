<!-- Source distribution -->
<assembly>
    <id>src</id>
    <formats>
        <format>tar.gz</format>
        <format>zip</format>
    </formats>
    <fileSets>
        <fileSet>
            <directory>.</directory>
            <useDefaultExcludes>true</useDefaultExcludes>
            <excludes>
                <exclude>**/target/**</exclude>
            </excludes>
        </fileSet>
    </fileSets>
</assembly>
