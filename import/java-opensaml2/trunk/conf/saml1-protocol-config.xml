<?xml version="1.0" encoding="UTF-8"?>
<XMLTooling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.opensaml.org/xmltooling-config ../../src/schema/xmltooling-config.xsd"
    xmlns:saml1p="urn:oasis:names:tc:SAML:1.0:protocol" 
    xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
    xmlns="http://www.opensaml.org/xmltooling-config">

    <!-- SAML 1.X Protocol Object providers -->

    <ObjectProviders>

        <!-- Response -->
        <ObjectProvider qualifiedName="saml1p:Response">
            <BuilderClass className="org.opensaml.saml1.core.impl.ResponseBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.ResponseMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.ResponseUnmarshaller" />
        </ObjectProvider>

        <!-- AssertionArtifact -->
        <ObjectProvider qualifiedName="saml1p:AssertionArtifact">
            <BuilderClass className="org.opensaml.saml1.core.impl.AssertionArtifactBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.AssertionArtifactMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.AssertionArtifactUnmarshaller" />
        </ObjectProvider>

        <!-- AuthenticationQuery -->
        <ObjectProvider qualifiedName="saml1p:AuthenticationQuery">
            <BuilderClass className="org.opensaml.saml1.core.impl.AuthenticationQueryBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.AuthenticationQueryMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.AuthenticationQueryUnmarshaller" />
        </ObjectProvider>

        <!-- AttributeQuery -->
        <ObjectProvider qualifiedName="saml1p:AttributeQuery">
            <BuilderClass className="org.opensaml.saml1.core.impl.AttributeQueryBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.AttributeQueryMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.AttributeQueryUnmarshaller" />
        </ObjectProvider>

        <!-- AuthorizationDecisionQuery -->
        <ObjectProvider qualifiedName="saml1p:AuthorizationDecisionQuery">
            <BuilderClass className="org.opensaml.saml1.core.impl.AuthorizationDecisionQueryBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.AuthorizationDecisionQueryMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.AuthorizationDecisionQueryUnmarshaller" />
        </ObjectProvider>

        <!-- Request -->
        <ObjectProvider qualifiedName="saml1p:Request">
            <BuilderClass className="org.opensaml.saml1.core.impl.RequestBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.RequestMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.RequestUnmarshaller" />
        </ObjectProvider>

        <!-- Status -->
        <ObjectProvider qualifiedName="saml1p:Status">
            <BuilderClass className="org.opensaml.saml1.core.impl.StatusBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.StatusMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.StatusUnmarshaller" />
        </ObjectProvider>

        <ObjectProvider qualifiedName="saml1p:StatusCode">
            <BuilderClass className="org.opensaml.saml1.core.impl.StatusCodeBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.StatusCodeMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.StatusCodeUnmarshaller" />
        </ObjectProvider>

        <ObjectProvider qualifiedName="saml1p:StatusMessage">
            <BuilderClass className="org.opensaml.saml1.core.impl.StatusMessageBuilder" />
            <MarshallingClass className="org.opensaml.saml1.core.impl.StatusMessageMarshaller" />
            <UnmarshallingClass className="org.opensaml.saml1.core.impl.StatusMessageUnmarshaller" />
        </ObjectProvider>

    </ObjectProviders>
</XMLTooling>